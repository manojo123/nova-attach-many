{
    "name": "manojo123/nova-attach-pivot",
    "description": "Attach Pivot Nova field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "NovaAttachPivot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaAttachPivot\\Providers\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
